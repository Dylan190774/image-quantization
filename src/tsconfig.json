{
	"compilerOptions" : {
		"outDir" : "../dist/",
		"sourceMap" : true,
		"module" : "commonjs",
		"moduleResolution" : "node",
		"target" : "es6",
		"noEmit" : true,
		"noEmitOnError" : true,
		"noFallthroughCasesInSwitch" : true,
		"noImplicitAny" : true,
		"noImplicitReturns" : true,
		"noImplicitThis" : true,
		"noUnusedLocals" : true,
		"noUnusedParameters" : false,
		"preserveConstEnums" : true,
		"pretty" : true,
		"strictNullChecks" : true,
		"forceConsistentCasingInFileNames" : true,
		"declaration" : true,
		"diagnostics" : true
	},
	"files" : [
		"./iq.ts"
	]
}
